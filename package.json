{
  "name": "farray's-international-dance-center",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run update:sitemap && vite build && node prerender.mjs",
    "build:images": "node scripts/build-images/index.mjs",
    "build:images:force": "node scripts/build-images/index.mjs --force",
    "build:images:class": "node scripts/build-images/index.mjs --class",
    "build:images:list": "node scripts/build-images/index.mjs --list",
    "create:class": "node scripts/create-class-page.mjs",
    "update:sitemap": "node scripts/update-sitemap.mjs",
    "indexnow": "node scripts/indexnow.mjs",
    "indexnow:all": "node scripts/indexnow.mjs --all",
    "indexnow:changed": "node scripts/indexnow.mjs --changed",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:a11y": "pa11y-ci",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:regression": "vitest run api/lib/__tests__/critical-systems.regression.test.ts --reporter=verbose",
    "test:pre-deploy": "npm run test:regression && npm run test:run && npm run build",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "size": "size-limit",
    "lighthouse": "lhci autorun",
    "csp:hash": "node scripts/generate-csp-hashes.mjs",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.73.0",
    "@sentry/react": "^10.23.0",
    "@supabase/supabase-js": "^2.93.3",
    "@types/dompurify": "^3.0.5",
    "@upstash/qstash": "^2.9.0",
    "@upstash/redis": "^1.36.1",
    "@vercel/kv": "^3.0.0",
    "dompurify": "^3.3.0",
    "googleapis": "^171.0.0",
    "hls.js": "^1.6.15",
    "i18next": "^25.8.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-resources-to-backend": "^1.2.1",
    "ioredis": "^5.8.2",
    "libphonenumber-js": "^1.12.34",
    "openai": "^6.21.0",
    "react": "^19.2.0",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^16.5.3",
    "react-router-dom": "^7.13.0",
    "resend": "^6.9.1",
    "web-vitals": "^5.1.0"
  },
  "overrides": {
    "body-parser": "^2.2.1",
    "express": "^5.1.0",
    "tmp": "^0.2.5",
    "esbuild": "^0.25.0",
    "path-to-regexp": "^8.0.0",
    "undici": "^6.0.0",
    "diff": "^8.0.3",
    "react-helmet-async": {
      "react": "$react"
    }
  },
  "devDependencies": {
    "@axe-core/react": "^4.11.0",
    "@chromatic-com/storybook": "^4.1.3",
    "@eslint/js": "^9.39.1",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.57.0",
    "@size-limit/file": "^11.2.0",
    "@size-limit/preset-app": "^11.2.0",
    "@storybook/addon-a11y": "^10.1.8",
    "@storybook/addon-docs": "^10.1.8",
    "@storybook/addon-onboarding": "^10.1.8",
    "@storybook/addon-vitest": "^10.1.8",
    "@storybook/react-vite": "^10.1.8",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^22.14.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vercel/node": "^5.5.15",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "cli-progress": "^3.12.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-storybook": "^10.1.8",
    "husky": "^9.1.7",
    "i18next-parser": "^9.3.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^27.1.0",
    "lint-staged": "^16.2.7",
    "p-limit": "^7.2.0",
    "pa11y-ci": "^4.0.1",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "react-dom": "^19.2.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "sharp": "^0.34.5",
    "storybook": "^10.1.8",
    "tailwindcss": "^3.4.18",
    "terser": "^5.44.1",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vite-imagetools": "^9.0.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^3.2.4",
    "workbox-window": "^7.4.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --max-warnings 0 --no-warn-ignored",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  }
}
