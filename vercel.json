{
  "buildCommand": "npm ci && npm run build",
  "installCommand": "npm ci",
  "outputDirectory": "dist",
  "framework": "vite",
  "cleanUrls": true,
  "trailingSlash": false,
  "functions": {
    "api/clases.ts": {
      "memory": 512,
      "maxDuration": 30
    },
    "api/cache-warm.ts": {
      "memory": 512,
      "maxDuration": 60
    },
    "api/reservar.ts": {
      "memory": 512,
      "maxDuration": 15,
      "includeFiles": "api/lib/**"
    },
    "api/cron-reminders-24h.ts": {
      "memory": 512,
      "maxDuration": 60
    },
    "api/cron-feedback.ts": {
      "memory": 512,
      "maxDuration": 60,
      "includeFiles": "api/lib/**"
    },
    "api/feedback.ts": {
      "memory": 512,
      "maxDuration": 10
    },
    "api/webhook-whatsapp.ts": {
      "memory": 512,
      "maxDuration": 30,
      "includeFiles": "api/lib/**"
    },
    "api/cancelar-reserva.ts": {
      "memory": 512,
      "maxDuration": 15,
      "includeFiles": "api/lib/**"
    },
    "api/attendance.ts": {
      "memory": 512,
      "maxDuration": 15,
      "includeFiles": "api/lib/**"
    },
    "api/test-calendar.ts": {
      "memory": 512,
      "maxDuration": 10,
      "includeFiles": "api/lib/**"
    },
    "api/cron-reminders.ts": {
      "memory": 512,
      "maxDuration": 60,
      "includeFiles": "api/lib/**"
    },
    "api/**/*.ts": {
      "memory": 512,
      "maxDuration": 60,
      "includeFiles": "api/lib/**"
    }
  },
  "crons": [
    {
      "path": "/api/cron-reminders",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/cron-reminders-24h",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/cron-feedback",
      "schedule": "30 * * * *"
    },
    {
      "path": "/api/cron-fichaje",
      "schedule": "*/5 * * * *"
    }
  ],
